def same_case(a, b): 
    # your code here
    if a.isalpha() == True and b.isalpha() == True: # Remember to include brackets in syntax
        if (a.isupper() == True and b.isupper() == True) or (a.islower() == True and b.islower() == True):
            return 1
        else:
            return 0
    
    return -1
